http:
  routers:
    mgt:
      entrypoints: websecure
      tls:
        certresolver: {{traefik_mgt_certresolver}}
      middlewares: auth-mgt@file
      rule: Host(`proxymgt.{{traefik_domain}}`)
      service: api@internal
  middlewares:
    auth-mgt:
      basicauth:
        users:
{% for line in traefik_mgt_users_formatted %}
          - "{{line}}"
{% endfor %}